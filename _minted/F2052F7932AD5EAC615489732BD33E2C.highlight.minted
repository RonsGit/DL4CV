			\PYG{n}{v} \PYG{o}{=} \PYG{l+m+mf}{1.0} \PYG{o}{/} \PYG{p}{(}\PYG{n}{A}\PYG{o}{.}\PYG{n}{T} \PYG{o}{@} \PYG{n}{u} \PYG{o}{+} \PYG{n}{eps}\PYG{p}{)}         \PYG{c+c1}{\PYGZsh{} column update (per sample)}
			\PYG{n}{u} \PYG{o}{=} \PYG{p}{(}\PYG{l+m+mf}{1.0} \PYG{o}{/} \PYG{n}{K}\PYG{p}{)} \PYG{o}{/} \PYG{p}{(}\PYG{n}{A} \PYG{o}{@} \PYG{n}{v} \PYG{o}{+} \PYG{n}{eps}\PYG{p}{)}     \PYG{c+c1}{\PYGZsh{} row update (equipartition)}
		
		\PYG{c+c1}{\PYGZsh{} Step 4: Reconstruct Q = diag(u) @ A @ diag(v)}
