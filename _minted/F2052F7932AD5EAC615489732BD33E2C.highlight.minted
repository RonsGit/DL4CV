\PYG{+w}{		}\PYG{l+s+sd}{\PYGZdq{}\PYGZdq{}\PYGZdq{}}
\PYG{l+s+sd}{		Sinkhorn\PYGZhy{}Knopp matrix balancing for prototype assignments.}
\PYG{l+s+sd}{		}
\PYG{l+s+sd}{		Args:}
\PYG{l+s+sd}{		logits : (K, B) teacher logits (K = prototypes, B = samples)}
\PYG{l+s+sd}{		tau    : temperature for sharpening (smaller \PYGZhy{}\PYGZgt{} peakier)}
\PYG{l+s+sd}{		n\PYGZus{}iter : number of Sinkhorn iterations (3â€“5 usually sufficient)}
\PYG{l+s+sd}{		eps    : small constant for numerical stability}
\PYG{l+s+sd}{		}
\PYG{l+s+sd}{		Returns:}
\PYG{l+s+sd}{		Q : (K, B) assignment matrix with row sums approx 1/K}
\PYG{l+s+sd}{		and column sums approx 1}
\PYG{l+s+sd}{		\PYGZdq{}\PYGZdq{}\PYGZdq{}}
		\PYG{n}{K}\PYG{p}{,} \PYG{n}{B} \PYG{o}{=} \PYG{n}{logits}\PYG{o}{.}\PYG{n}{shape}
		
		\PYG{c+c1}{\PYGZsh{} Step 1: Compute affinity matrix A (unnormalized)}
		\PYG{n}{A} \PYG{o}{=} \PYG{n}{np}\PYG{o}{.}\PYG{n}{exp}\PYG{p}{(}\PYG{n}{logits} \PYG{o}{/} \PYG{n}{tau}\PYG{p}{)}
		
		\PYG{c+c1}{\PYGZsh{} Step 2: Initialize scaling vectors}
		\PYG{n}{u} \PYG{o}{=} \PYG{n}{np}\PYG{o}{.}\PYG{n}{ones}\PYG{p}{(}\PYG{p}{(}\PYG{n}{K}\PYG{p}{,} \PYG{l+m+mi}{1}\PYG{p}{)}\PYG{p}{,} \PYG{n}{dtype}\PYG{o}{=}\PYG{n}{A}\PYG{o}{.}\PYG{n}{dtype}\PYG{p}{)} \PYG{o}{/} \PYG{n}{K}  \PYG{c+c1}{\PYGZsh{} target row mass}
		\PYG{n}{v} \PYG{o}{=} \PYG{n}{np}\PYG{o}{.}\PYG{n}{ones}\PYG{p}{(}\PYG{p}{(}\PYG{n}{B}\PYG{p}{,} \PYG{l+m+mi}{1}\PYG{p}{)}\PYG{p}{,} \PYG{n}{dtype}\PYG{o}{=}\PYG{n}{A}\PYG{o}{.}\PYG{n}{dtype}\PYG{p}{)}      \PYG{c+c1}{\PYGZsh{} target column mass}
		
		\PYG{c+c1}{\PYGZsh{} Step 3: Iterative normalization}
