		\PYG{c+c1}{\PYGZsh{} Optional: normalize Q to sum to 1}
		\PYG{n}{Q} \PYG{o}{/}\PYG{o}{=} \PYG{n}{Q}\PYG{o}{.}\PYG{n}{sum}\PYG{p}{(}\PYG{p}{)}
		
		\PYG{k}{return} \PYG{n}{Q}
